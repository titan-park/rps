<launch>
	<node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen">
		<param name="pixel_format" value="yuyv" />
	</node>

	<node name="hcircle" pkg="opencv_apps" type="hough_circles" output="screen">
		<remap from="image" to="/usb_cam/image_raw"/>
	</node>

	<node name="rps_core" pkg="rps" type="rps_core.py" output="screen">
	</node>

	<node name="SerialSend" pkg="rps" type="SerialSend.py" output="screen">
	</node>

</launch>
